{
    "manifest_version": 2,
    "name": "FB Group Leads",
    "version": "1.0.0",
    "description": "Get group leads from fb to google sheet by google forms",
    "icons": {
        "128": "images/icon_128.png"
    },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "author": "Hasanuzzaman",
    "options_page": "options.html",
    "permissions": [
        "contextMenus",
        "tabs",
        "storage",
        "http://*/docs.google.com/forms/*",
        "https://*/facebook.com/*"
    ],
    "short_name": "FBGL"
}