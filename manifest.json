{
    "manifest_version": 2,
    "name": "FF Admin",
    "version": "1.0.0",
    "description": "Get Admin data",
    "icons": {
        "128": "images/icon_128.png"
    },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "author": "Hasanuzzaman",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "http://*/",
        "https://*/"
    ],
    "short_name": "FF"
}